#include <cstdio>
#include "grackle.h"
#include "auto_general.hpp"

extern "C" grackle_version get_grackle_version(void) {
  grackle_version out;
  out.version = "@VERSION_NUM@";
  out.branch = "@GIT_BRANCH@";
  out.revision = "@GIT_REVISION@";
  return out;
}

void auto_show_flags(std::FILE *fp) {
  std::fprintf (fp, "%s\n", "@SHOW_FLAGS_STR@");
}

void auto_show_config(std::FILE *fp) {
  std::fprintf (fp, "%s\n", "@SHOW_CONFIG_STR@");
}
